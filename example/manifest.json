{
  "manifest_version": 3,
  "name": "Volume Booster - CodeHemu",
  "version": "1.0",
  "description": "Boost tab audio up to 300%",
  "permissions": ["activeTab", "tabCapture", "tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_start"
  }]
}
