{
  "name": "volume-booster",
  "version": "1.2.4",
  "description": "Unleash the power of sound on your browser!",
  "homepage": "https://github.com/code-hemu/volume-booster#readme",
  "bugs": {
    "url": "https://github.com/code-hemu/volume-booster/issues"
  },
  "json.schemaValidation": "off",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-hemu/volume-booster.git"
  },
  "license": "GPLv3",
  "author": "Hemanta Gayen",
  "type": "module",
  "engines": {
    "node": ">=22",
    "npm": ">=11"
  },
  "scripts": {
    "prebuild": "node --max-old-space-size=3072 tasks/cli.js zip",
    "chrome": "node --max-old-space-size=3072 tasks/cli.js build --chrome --release",
    "opera": "node --max-old-space-size=3072 tasks/cli.js build --opera --release",
    "never": "node --max-old-space-size=3072 tasks/cli.js build --naver --release",
    "edge": "node --max-old-space-size=3072 tasks/cli.js build --edge --release",
    "help": "node --max-old-space-size=3072 tasks/cli.js --help",
    "build": "node --max-old-space-size=3072 tasks/cli.js build --all --release",
    "debug": "node --max-old-space-size=3072 tasks/cli.js build --all --debug",
    "watch": "node --max-old-space-size=3072 tasks/cli.js build --all --debug --watch",
    "zip": "node --max-old-space-size=3072 tasks/cli.js zip",
    "chrome:watch": "node --max-old-space-size=3072 tasks/cli.js build --chrome --release --watch"
  },
  "devDependencies": {
    "@craftamap/esbuild-plugin-html": "^0.9.0",
    "chokidar": "^5.0.0",
    "esbuild": "^0.27.3",
    "esbuild-sass-plugin": "^3.6.0",
    "globby": "^16.1.0",
    "yazl": "^3.3.1",
    "sass": "^1.97.3"
  }
}
